{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "no-console": "off",
        "no-eval": "off",
        "no-useless-escape": "off",
        "no-unused-vars": "warn",
        "react/prop-types": "off",
        "react/jsx-key": "warn",
        "react/no-unescaped-entities": "warn",
        "react/no-unknown-property": "warn"
    }
}
